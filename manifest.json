{
    "manifest_version": 2,
    "name": "F1TV English",
    "version": "0.1.1",
    "description": "Listen to english commentary in any country",
    "background": {
        "scripts": ["background.js"],
        "persistent": true
      },
    "permissions": [
        "https://*.formula1.com/*",
        "webRequest",
        "webRequestBlocking",
        "activeTab"        
    ],
    "icons": { 
      "16": "icon16.png",
      "32": "icon32.png",
      "192": "icon192.png"
    }
  }
